<%= form_with model: object, url: resource_path do |f| %>
  <%= f.text_field :username, placeholder: "user_name" %>
  <br>
  <%= f.email_field :email, placeholder: "user_email@example.com" %>
  <br>
  <%= f.password_field :password, placeholder: "password"%>
  <br>
  <%= f.password_field :password_confirmation, placeholder: "password confirmation" %>
  <br>
  <%= f.submit %>
<% end %>
